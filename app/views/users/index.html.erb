<h2>All Users</h2>

<table class="table table-striped">
		<thead class="thead-dark">
			<tr>
				<th>Username</th>
				<th>Votes</th>
				<th>Joined</th>
			</tr>
			</thead>

			<% @users.each do |user| %>
				<tr>
					<td> 
						<%= user.username %>
					</td>
					<td>
						<%= user.votes.count %>
					</td>
					<td>
						<%= user.created_at.strftime("%m-%d-%Y") %>
					</td>
				</tr>
			<% end %>

	</table>